<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_background"
    tools:context=".MaidProfile1">

    <TextView
        android:id="@+id/Maid3Profile"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:text="Maid 3 Profile"
        android:textColor="@color/colorPrimary"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.120000005" />

    <TextView
        android:id="@+id/Maid3Name"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="Name"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/Maid3Profile"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/Maid3Profile"
        app:layout_constraintTop_toBottomOf="@+id/Maid3Profile" />

    <TextView
        android:id="@+id/Maid3Tania"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Tania"
        android:textSize="21sp"
        app:layout_constraintEnd_toEndOf="@+id/Maid3Name"
        app:layout_constraintStart_toStartOf="@+id/Maid3Name"
        app:layout_constraintTop_toBottomOf="@+id/Maid3Name" />

    <TextView
        android:id="@+id/Maid3age"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Age"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/Maid3Tania"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/Maid3Tania"
        app:layout_constraintTop_toBottomOf="@+id/Maid3Tania" />

    <TextView
        android:id="@+id/maid3age22"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="22"
        android:textSize="21sp"
        app:layout_constraintEnd_toEndOf="@+id/Maid3age"
        app:layout_constraintStart_toStartOf="@+id/Maid3age"
        app:layout_constraintTop_toBottomOf="@+id/Maid3age" />

    <TextView
        android:id="@+id/Maid3Experince"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Experince"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/maid3age22"
        app:layout_constraintStart_toStartOf="@+id/maid3age22"
        app:layout_constraintTop_toBottomOf="@+id/maid3age22" />

    <TextView
        android:id="@+id/Maid3Ex7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="7 Years"
        android:textSize="21sp"
        app:layout_constraintEnd_toEndOf="@+id/Maid3Experince"
        app:layout_constraintStart_toStartOf="@+id/Maid3Experince"
        app:layout_constraintTop_toBottomOf="@+id/Maid3Experince" />

    <TextView
        android:id="@+id/Maid3Address"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Address"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/Maid3Ex7"
        app:layout_constraintStart_toStartOf="@+id/Maid3Ex7"
        app:layout_constraintTop_toBottomOf="@+id/Maid3Ex7" />

    <TextView
        android:id="@+id/Maid3Ad"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Allama Shabbir Ahmed Usmani Rd, Block 7 Gulshan-e-Iqbal, Karachi, Karachi City, Sindh, Pakistan"
        android:textSize="21sp"
        app:layout_constraintEnd_toEndOf="@+id/Maid3Address"
        app:layout_constraintStart_toStartOf="@+id/Maid3Address"
        app:layout_constraintTop_toBottomOf="@+id/Maid3Address" />

    <Button
        android:id="@+id/Maid3btnBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Back"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Maid3btnSelect"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toBottomOf="@+id/Maid3Ad"
        app:layout_constraintVertical_bias="0.06999999" />

    <Button
        android:id="@+id/Maid3btnSelect"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:text="Select Maid"
        android:textColor="@color/colorPrimary"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/Maid3btnBack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/Maid3btnBack"
        app:layout_constraintTop_toBottomOf="@+id/Maid3Ad"

        />
</androidx.constraintlayout.widget.ConstraintLayout>